{
  "name": "simple-soccer-detector",
  "version": "1.0.0",
  "description": "Simple Soccer Detector Desktop App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run start\" \"npm run watch\"",
    "watch": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "dist": "npm run build && electron-builder"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {

    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron-builder": "^26.0.12",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "build": {
  "appId": "com.example.simplesoccerdetector",
  "productName": "Simple Soccer Detector",
  "asarUnpack": [
      "main.py",
      "tools/**/*"
    ],
  "files": [
    "main.js",
    "main.html",
    "preload.js",
    "analyze.html",
    "main.py",
    "dist/**/*",
    "src/**/*",
    "static/**/*",
    "output/**/*",
    "tools/**/*",
    "webpack.config.js"
  ]
,
  "mac": {
    "target": "dmg"
  },
  "win": {
    "target": "nsis"
  }
}
}
